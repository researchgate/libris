{
  "name": "researchgate/libris",
  "description": "An RIS parser. RIS is a format for reference metadata. A fork of technosophos/LibRIS.",
  "type": "library",
  "keywords": [
    "RIS",
    "File parser",
    "Reference",
    "Bibliography"
  ],
  "license": "MIT or GPLv2",
  "homepage": "https://github.com/thompsonsj/LibRIS",
  "authors": [
    {
      "name": "M Butcher",
      "homepage": "http://technosophos.com"
    }
  ],
  "autoload": {
    "psr-0": {
      "LibRIS\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LibRIS\\": "tests/"
    }
  },
  "require": {
    "php": ">= 7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phan/phan": "^5.2",
    "friendsofphp/php-cs-fixer": "^3.1",
    "vimeo/psalm": "^4.23",
    "phpstan/phpstan": "^1.7"
  }
}
